/*
f is control bits
000: out = a AND b
001: out = a OR b
010: out = a + b
011: not used
100: out = a AND NOT(b)
101: out = a OR NOT(b)
110: out = a - b
111: SLT (SET LESS THAN) out is 1 if a < b else 0
*/

CHIP MiniALU {
    IN
        a[16], b[16], f[3];  // 16-bit inputs and control bits

    OUT
        out[16]; // 16-bit output

    PARTS:
}
